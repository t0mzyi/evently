                    <div><%= tickets[i].event.startDate.time %></div>
                    <div class="hidden md:block w-1 h-1 rounded-full bg-current"></div>
                    <div><%= tickets[i].event.venue %></div>
                  </div>
                  <div class="flex items-center gap-2 text-[#1C2A3A]/60 text-sm font-sans">
