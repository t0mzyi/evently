                  <h3 class="text-2xl md:text-3xl font-heading text-[#1C2A3A] mb-1 leading-tight hover:text-[#2C3E50] cursor-pointer transition-colors">
                    <a href="/view-past-event/<%= tickets[i].event.id %>"><%= tickets[i].event.title %></a>
